function addCookie(){if(document.all){window.external.addFavorite("http://www.scholat.com","学者网")}else{if(window.sidebar){window.sidebar.addPanel("学者网","http://www.scholat.com","")}}}function setHomepage(){if(document.all){document.body.style.behavior="url(#default#homepage)";document.body.setHomePage("http://www.scholat.com")}else{if(window.sidebar){if(window.netscape){try{netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect")}catch(b){alert("所用浏览器不支持该功能")}}var a=Components.classes["@mozilla.org/preferences-service;1"].getService(Components.interfaces.nsIPrefBranch);a.setCharPref("browser.startup.homepage","http://www.scholat.com")}}}function Requestforaddfriend(a,b){if(confirm("确定加为好友？")){$.ajax({type:"post",url:"Requestforaddfriend.html",timeout:60000,data:{receiverID:a,username:b},dataType:"text",cache:false,success:function(c){if(c=="you are friends"){window.alert("您们已经是朋友了！")}else{if(c=="you had send a request"){window.alert("您已经发送过申请！请等待好友答复！")}else{if(c=="yourself"){window.alert("不能添加自己或管理员为好友！")}else{window.alert("申请已发，请等待好友答复！")}}}}})}}function leaveMessageConfirm(){if(window.confirm("请先登录！")){window.location.href="scholarLeaveMessage.html?Entry=${Entry}"}}function getuniversitylist(){$.ajax({type:"get",url:"university.html",timeout:60000,dataType:"text",cache:false,success:function(b){var a=b.split(",");$("#portalregister_profile_workUnit").autocomplete(a,{delay:720,max:100,width:254,matchContains:true})},error:function(){}})}function getuserclasslist(b,a){$.ajax({type:"post",url:"portalGetScholarbyClassfy.html",timeout:60000,dataType:"text",data:{classtype:b,num:a},cache:false,success:function(c){$("#divuserlist_"+b).html(c)},error:function(){}})}function getuserclasslist_all(b,a){$.ajax({type:"post",url:"portalGetScholarbyClassfy.html",timeout:60000,dataType:"text",data:{classtype:b,num:a},cache:false,success:function(c){$("#divframe_right").html(c)},error:function(){}})};